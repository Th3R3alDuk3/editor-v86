{
    "TinyCore": {
        "starter": {
            "bios": {
                "url": "bios/seabios_min.bin",
                "size": 65536,
                "async": false
            },
            "hda": {
                "url": "bootables/tinycore.raw",
                "async": false
            }
        },
        "listener": [
            {
                "receive": "box login:",
                "send": "tc"
            },
            {
                "receive": "tc@box:~$",
                "send": null
            }
        ],
        "applications": {
            "tcc": {
                "file": "out.c",
                "send": "tcc -run ./out.c"
            },
            "gcc": {
                "file": "out.c",
                "send": "gcc ./out.c -o ./out; ./out"
            },
            "python": {
                "file": "out.py",
                "send": "python2 ./out.py"
            }
        }
    },
    "TinyCore Minimal": {
        "starter": {
            "bios": {
                "url": "bios/seabios_min.bin",
                "size": 65536,
                "async": false
            },
            "hda": {
                "url": "bootables/tinycore_min.raw",
                "async": false
            }
        },
        "listener": [
            {
                "receive": "box login:",
